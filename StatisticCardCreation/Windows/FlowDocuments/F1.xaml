<Window x:Class="StatCardApp.DocumentViewerF1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:localControl="clr-namespace:StatCardApp.DerivedControls"
        xmlns:localConverter="clr-namespace:StatCardApp.Convertors"
        mc:Ignorable="d"
        Title="Ф-1 Документ"  Width="850" Height="60 cm" ResizeMode="NoResize" WindowState="Maximized">
    <Window.Resources>
        <localConverter:ValueToSizeConverter x:Key="ValueToSizeConverter" />
        <localConverter:DateConverter x:Key="DateConverter" />
        <localConverter:KeyValueConverter x:Key="KeyValueConverter" />
        <localConverter:QualificationConverter x:Key="QualificationConverter" />
        <localConverter:TimeConverter x:Key="TimeConverter" />
        <localControl:AutoSizeTextBox x:Key="AutoSizeTextBox" />

        <Style x:Key="mainTable" TargetType="TableCell">
            <Setter Property="BorderThickness" Value="2" />
            <Setter Property="BorderBrush" Value="Black" />
        </Style>

        <Style TargetType="TableCell">
            <Setter Property="BorderBrush" Value="Black" />
            <Setter Property="BorderThickness" Value="0, 0.5, 0.5, 0" />
            <Setter Property="Padding" Value="3,0,0,0" />
        </Style>

        <Style TargetType="Table">
            <Setter Property="CellSpacing" Value="0" />
            <Setter Property="Padding" Value="0" />
            <Setter Property="Margin" Value="0" />
        </Style>
        <Style TargetType="Paragraph">
            <Setter Property="FontSize" Value="0.0972 in" />
            <Setter Property="FontFamily" Value="Arial" />
            <Setter Property="TextAlignment" Value="Left" />
            <Setter Property="Margin" Value="0" />
        </Style>
        <Style x:Key="ValueStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="0.194 in" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Right" />
            <Setter Property="LineStackingStrategy" Value="MaxHeight" />
            <Setter Property="LineHeight" Value="12" />
            <Setter Property="FontFamily" Value="Times New Roman" />
        </Style>
        <Style TargetType="DockPanel">
            <Setter Property="Width" Value="670" />
        </Style>
        <Style TargetType="TextBlock">
            <Setter Property="LineStackingStrategy" Value="BlockLineHeight" />
            <Setter Property="LineHeight" Value="0.0902 in" />
            <Setter Property="Margin" Value="0,2,0,0" />
            <Setter Property="TextWrapping" Value="Wrap" />
        </Style>
    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <FlowDocumentScrollViewer Name="FlowDoc" Width="835"  HorizontalAlignment="Center" Margin="0,10,0,42" Grid.Row="0">
            <FlowDocument ColumnWidth="23 cm" Name ="docViewer">

                <!--#region Заголовок-->

                <Paragraph TextAlignment="Center"  FontSize="0.2 in" FontFamily="Times New Roman" Margin="0,0,0,0">
                    <StackPanel Orientation="Horizontal"  Height="58" HorizontalAlignment="Left" Width="705">
                        <TextBlock LineStackingStrategy="MaxHeight" TextAlignment="Left" Width="57" FontSize="0.138 in" FontWeight="Bold" FontFamily="Arial">Ф-1</TextBlock>
                        <StackPanel Width="576">
                            <TextBlock LineStackingStrategy="MaxHeight" FontWeight="Bold" >Статистическая карточка</TextBlock>
                            <TextBlock LineStackingStrategy="MaxHeight" LineHeight="14" FontSize="0.1805 in" Margin="0, 0, 0, 5" >на выявленное преступление, происшествие</TextBlock>
                        </StackPanel>
                        <StackPanel Width="69">
                            <TextBlock  FontSize="0.083 in">П р  и л о ж е н и е 1</TextBlock>
                            <TextBlock LineStackingStrategy="MaxHeight" FontSize="0.416 in" FontWeight="Bold">Ф-1</TextBlock>
                        </StackPanel>
                    </StackPanel>

                </Paragraph>
                <!--#endregion-->
                <!--#region Таблица "учесть"-->

                <Table BorderBrush="White" BorderThickness="0">
                    <Table.Columns>
                        <TableColumn Width="0.7*" />
                        <TableColumn Width="15*" />
                        <TableColumn Width="1.3*" />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow>
                            <TableCell Padding="4,4,0,0" BorderThickness="0">
                                <Paragraph FontSize="0.1388 in" FontFamily="Arial">
                                    <TextBlock>1</TextBlock>
                                </Paragraph>
                            </TableCell>
                            <!--#region Главная таблица-->
                            <TableCell Style="{StaticResource mainTable}">
                                <Paragraph  Margin="5" FontFamily="Arial">
                                    <DockPanel Height="13">
                                        <TextBlock Height="12" >Учесть: основное(1), дополнительное(2); учтено по другому делу(3); изменить(9)</TextBlock>
                                        <TextBlock TextAlignment="Right" Text="{Binding F1.Accounting}" FontSize="14" FontWeight="Bold"></TextBlock>
                                    </DockPanel>
                                </Paragraph>
                            </TableCell>
                            <!--#endregion-->
                            <TableCell  BorderThickness="0">
                                <Paragraph FontSize="0.1527 in" FontFamily="Times New Roman">
                                    <TextBlock>1</TextBlock>
                                </Paragraph>
                            </TableCell>
                        </TableRow>
                    </TableRowGroup>
                </Table>
                <!--#endregion-->
                <Paragraph Margin="45,5,0,1">
                    <TextBlock FontSize="0.138 in" FontFamily="Times New Roman">
                        <Bold FontSize="0.166 in">Раздел 1.</Bold> Заполняется работником ВП по ведению учетно-регистрационной работы
                    </TextBlock>
                </Paragraph>
                <!--#region Таблица первого раздела-->

                <Table>
                    <Table.Columns>
                        <TableColumn Width="0.7*" />
                        <TableColumn Width="14*" />
                        <TableColumn Width="1.3*" />
                    </Table.Columns>
                    <TableRowGroup>
                        <!--#region Cтили-->
                        <!--#endregion-->
                        <TableRow>
                            <!--#region Столбец с пунктами-->
                            <TableCell BorderBrush="White">
                                <Paragraph  FontSize="0.1388 in" FontFamily="Arial">
                                    <StackPanel>
                                        <TextBlock Height="37">1.1</TextBlock>
                                        <TextBlock Height="53">1.2</TextBlock>
                                        <TextBlock Height="22">1.3</TextBlock>
                                        <TextBlock Height="25">1.4</TextBlock>
                                        <TextBlock Height="27">1.5</TextBlock>
                                        <TextBlock>1.6</TextBlock>
                                    </StackPanel>
                                </Paragraph>
                            </TableCell>
                            <!--#endregion-->
                            <!--#region Столбец с главной таблицей-->
                            <TableCell Style="{StaticResource mainTable}">
                                <Table>
                                    <Table.Columns>
                                        <TableColumn Width="1.5*" />
                                        <TableColumn Width="3*" />
                                        <TableColumn Width="3*" />
                                    </Table.Columns>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell BorderThickness="0, 0, 0.5, 0.5">
                                                <Paragraph>
                                                    <TextBlock Height="25" Width="132">
                                                        Номер уголовного <Bold>Д</Bold>ела (<Bold>М</Bold>атериала, <Bold>И</Bold>нформации)
                                                    </TextBlock>
                                                </Paragraph>
                                            </TableCell>
                                            <TableCell BorderThickness="0, 0, 0.5, 0.5">
                                                <Paragraph FontFamily="Times New Roman">
                                                    <TextBlock Margin="0,3,0,0" Width="270" Height="26">
                                                        1 – дело, 2 – материал, 3 – дело частного обвинения,  <LineBreak />
                                                        4 – выделенное из другого дела,
                                                        5 – информация
                                                    </TextBlock>
                                                </Paragraph>
                                            </TableCell>
                                            <TableCell BorderThickness="0, 0, 0.5, 0.5" Background="LightGray">
                                                <Paragraph>
                                                    <DockPanel Width="271">
                                                        <TextBlock LineStackingStrategy="MaxHeight" Text="{Binding FullCaseNumber}" FontSize="0.25 in" FontFamily="Times New Roman" />
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                    </TableRowGroup>
                                </Table>
                                <Table>
                                    <Table.Columns>
                                        <TableColumn Width="1.5*" />
                                        <TableColumn Width="2*" />
                                        <TableColumn Width="1*" />
                                        <TableColumn Width="1*" />
                                    </Table.Columns>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell ColumnSpan="2" BorderThickness="0">
                                                <Paragraph>
                                                    <StackPanel Orientation="Horizontal" Width="435">
                                                        <TextBlock Width="188"><Bold>За кем числится</Bold> преступление:</TextBlock>
                                                        <TextBlock HorizontalAlignment="Right">а) УСЛОВНОЕ наименование воинской части <LineBreak /> (соединения), учреждения, организации</TextBlock>
                                                    </StackPanel>
                                                </Paragraph>
                                            </TableCell>
                                            <TableCell ColumnSpan="2" BorderThickness="0.5, 0, 0, 0.5" Padding="1,1,0,1">
                                                <Paragraph>
                                                    <Border BorderBrush="Black" BorderThickness="0.5,0.5,0,0.5" Width="250" Height="24">
                                                        <DockPanel Width="Auto" Margin="2,0,12,0">
                                                            <localControl:AutoSizeTextBox Text="{Binding F1.ListedCrimeObject}" FontSize="0.194 in"  Margin="0" Padding="2,0,0,0" />
                                                        </DockPanel>
                                                    </Border>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                    </TableRowGroup>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <TextBlock TextWrapping="Wrap">
                                                        <Bold><Underline><Italic>ПРИМЕЧАНИЕ</Italic></Underline></Bold>:
                                                        строка 1.2 при совершении преступления только гражданскими лицами <Bold><Underline><Italic>не заполняется!</Italic></Underline></Bold>
                                                    </TextBlock>
                                                </Paragraph>
                                            </TableCell>
                                            <TableCell ColumnSpan="2" BorderThickness="0">
                                                <Paragraph>
                                                    <TextBlock>
                                                        д) Наименование военной прокуратуры округа, флота, вида войск, за которой
                                                    <LineBreak /> числится настоящее преступление
                                                    </TextBlock>
                                                    <TextBlock FontSize="7"><Italic> (заполняется, если часть не поднадзорна данной ВП окружного звена)</Italic></TextBlock>
                                                </Paragraph>
                                            </TableCell>
                                            <TableCell BorderThickness="0.5, 0, 0, 0" Padding="1,0,0,0">
                                                <Paragraph>
                                                    <Border BorderBrush="Black" BorderThickness="0.5,0,0,0.5" Width="121">
                                                        <localControl:AutoSizeTextBox Text="{Binding F1.ListedProsecutorOffice}"  Width="121" Padding="2,0,0,0" Margin="0"  FontSize="0.222 in" FontFamily="Times New Roman" Height="28" />
                                                    </Border>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                    </TableRowGroup>
                                </Table>
                                <Table>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <DockPanel>
                                                        <TextBlock>Порядковый номер преступления в уголовном деле</TextBlock>
                                                        <TextBlock Text="{Binding F1.CrimeNumber, Converter={StaticResource ValueToSizeConverter}, ConverterParameter='3'}"
                                                                   Style="{StaticResource ValueStyle}" />
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                    </TableRowGroup>
                                </Table>
                                <Table>
                                    <Table.Columns>
                                        <TableColumn Width="6*" />
                                        <TableColumn Width="1*" />
                                    </Table.Columns>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <DockPanel Width="592" Height="30">
                                                        <TextBlock>Номер регистрации заявления, сообщения о преступлении или информации и дата регстрации:</TextBlock>
                                                        <StackPanel Width="93" HorizontalAlignment="Right">
                                                            <TextBlock Text="номер" HorizontalAlignment="Center" FontSize="0.083 in" FontFamily="Times New Roman" Width="24" />
                                                            <TextBlock HorizontalAlignment="Center" Text="{Binding F1.StateRegNumber, Converter={StaticResource ValueToSizeConverter},
                                                                ConverterParameter='3'}" Style="{StaticResource ValueStyle}" FontWeight="Normal" />
                                                        </StackPanel>
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
                                            <TableCell Padding="0">
                                                <Paragraph>
                                                    <StackPanel Width="94" Height="30">
                                                        <TextBlock Text="число     месяц     год" HorizontalAlignment="Center" FontSize="0.083 in" FontFamily="Times New Roman" />
                                                        <TextBlock HorizontalAlignment="Center" Text="{Binding F1.RegistrationDate, Converter={StaticResource DateConverter}, ConverterParameter=ShortYear}"
                                                                   Style="{StaticResource ValueStyle}" FontWeight="Normal" />
                                                    </StackPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                    </TableRowGroup>
                                </Table>
                                <Table>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <StackPanel Orientation="Horizontal" Height="30" Width="680">
                                                        <TextBlock Width="577">Дата направления карточки прокурору:</TextBlock>
                                                        <StackPanel Width="100">
                                                            <TextBlock Text="число     месяц     год" HorizontalAlignment="Center" FontSize="0.083 in" FontFamily="Times New Roman" />
                                                            <TextBlock HorizontalAlignment="Center" Text="{Binding F1.TransCardDate, Converter={StaticResource DateConverter}, ConverterParameter=ShortYear}"
                                                                       Style="{StaticResource ValueStyle}" FontWeight="Normal" />
                                                        </StackPanel>
                                                    </StackPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <DockPanel>
                                                        <TextBlock Height="19">Дата учета данных карточки в ИПК "Преступность"</TextBlock>
                                                        <TextBlock />
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                    </TableRowGroup>
                                </Table>
                            </TableCell>
                            <!--#endregion-->
                            <!--#region Столбец с пунктами -->
                            <TableCell BorderBrush="White">
                                <Paragraph FontSize="0.1527 in" FontFamily="Times New Roman">
                                    <StackPanel Margin="0,1,0,0">
                                        <TextBlock Height="36">1.1</TextBlock>
                                        <TextBlock Height="25">1.2.а</TextBlock>
                                        <TextBlock Height="29">1.2.д</TextBlock>
                                        <TextBlock Height="21">1.3</TextBlock>
                                        <TextBlock Height="26">1.4</TextBlock>
                                        <TextBlock Height="26">1.5</TextBlock>
                                        <TextBlock>1.6</TextBlock>
                                    </StackPanel>
                                </Paragraph>
                            </TableCell>
                            <!--#endregion-->
                        </TableRow>
                    </TableRowGroup>
                </Table>
                <!--#endregion-->
                <Paragraph Margin="45,5,0,1">
                    <TextBlock FontSize="0.138 in" FontFamily="Times New Roman">
                        <Bold FontSize="0.166 in">Раздел 2.</Bold> Заполняется лицом, ведущим расследование уголовного дела или разрешившим материалы
                    </TextBlock>
                </Paragraph>
                <!--#region Таблица второго раздела-->
                <Table>
                    <Table.Columns>
                        <TableColumn Width="0.7*" />
                        <TableColumn Width="14*" />
                        <TableColumn Width="1.3*" />
                    </Table.Columns>
                    <TableRowGroup>
                        <TableRow>
                            <!--#region Столбец с пунктами-->
                            <TableCell BorderThickness="0">
                                <Paragraph FontSize="0.1388 in" FontFamily="Arial">
                                    <StackPanel>
                                        <TextBlock Height="89" Margin="0,5,0,0">2.1</TextBlock>
                                        <TextBlock Height="18">2.2</TextBlock>
                                        <TextBlock Height="28">2.3</TextBlock>
                                        <TextBlock Height="66">2.4</TextBlock>
                                        <TextBlock Height="96">2.5</TextBlock>
                                        <TextBlock Height="96">2.6</TextBlock>
                                        <TextBlock Height="130">2.7</TextBlock>
                                        <TextBlock>2.8</TextBlock>
                                    </StackPanel>
                                </Paragraph>
                            </TableCell>
                            <!--#endregion-->
                            <!--#region Столбец основной таблицы-->
                            <TableCell Style="{StaticResource mainTable}">
                                <Table>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <DockPanel HorizontalAlignment="Stretch">
                                                        <TextBlock>
                                                            <Bold>Сведения о нарушениях УПК РФ и Дисциплинарного устава:</Bold>
                                                            <LineBreak />преступление сокрыто (1), решение не принято (2), принято несвоевременно
                                                            <LineBreak />Копия постановления о возбуждении дела не направлена ВП (4), направлена ВП несвоевременно;
                                                            <LineBreak />нарушен срок производства неотложных следственных действий (6), иные нарушения УПК (9)
                                                        </TextBlock>
                                                        <TextBlock Text="{Binding F1.ViolationInfo}" Style="{StaticResource ValueStyle}" HorizontalAlignment="Right" />
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <DockPanel>
                                                        <TextBlock Width="602">
                                                            <Bold>кем допущены нарушения:</Bold>командиром части (60), дознавателем (62), военной полицией (63), иным воинским должностным лицом (66);
                                                            <LineBreak /><Underline>следственными органами СК</Underline> военным (01), территориальными (02), <Underline>сотрудниками</Underline>
                                                            УР(21), ОБЭП (22), наркоконтроля ОВД (23), следственных органов МВД (25), иными ОВД (20)
                                                            ФСКН (32), таможни (35), ФСБ (40), ВКР ФСБ (41), ФССП (52), пожарной службы (55)
                                                        </TextBlock>
                                                        <TextBlock Text="{Binding F1.CrimePerson, Converter={StaticResource ValueToSizeConverter}, ConverterParameter=2}" Style="{StaticResource ValueStyle}" />
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                    </TableRowGroup>
                                </Table>
                                <Table>
                                    <Table.Columns>
                                        <TableColumn />
                                        <TableColumn />
                                        <TableColumn />
                                    </Table.Columns>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell Padding="0">
                                                <Paragraph>
                                                    <StackPanel Width="230">
                                                        <TextBlock LineHeight="5" VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="Times New Roman" FontSize="0.083 in" Padding="0,0,0,2">должность:</TextBlock>
                                                        <localControl:AutoSizeTextBox Text="{Binding F1.CrimePersDescription.Position}" HorizontalAlignment="Center" Height="19" />
                                                    </StackPanel>
                                                </Paragraph>
                                            </TableCell>
                                            <TableCell>
                                                <Paragraph>
                                                    <StackPanel Width="226">
                                                        <TextBlock LineHeight="5" VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="Times New Roman" FontSize="0.083 in" Padding="0,0,0,2">Звание:</TextBlock>
                                                        <localControl:AutoSizeTextBox Text="{Binding F1.CrimePersDescription.Rank}" HorizontalAlignment="Center" Height="19"/>
                                                    </StackPanel>
                                                </Paragraph>
                                            </TableCell>
                                            <TableCell>
                                                <Paragraph>
                                                    <StackPanel Width="226">
                                                        <TextBlock LineHeight="5" VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="Times New Roman" FontSize="0.083 in" Padding="0,0,0,2">Фамилия, инициалы:</TextBlock>
                                                        <localControl:AutoSizeTextBox Text="{Binding F1.CrimePersDescription.FIO}" HorizontalAlignment="Center" Height="19"/>
                                                    </StackPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                    </TableRowGroup>
                                </Table>
                                <Table>
                                    <TableRowGroup>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <DockPanel>
                                                        <TextBlock>
                                                            <Bold>Преступление требует раскрытия (раскрыто)</Bold>: следственным путем (не раскрыто) (1), раскрыто: следователем ВСО в ходе доследственной
                                                            <LineBreak />проверки (2), командованием (3), ОВД (4), ФСБ (5), территориальным СО СК РФ (6), ФСКН (7), другим ВСО (8) иным органом (9)
                                                        </TextBlock>
                                                        <TextBlock Text="{Binding F1.DetectionRequirement}" Style="{StaticResource ValueStyle}" />
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>

                                                    <DockPanel>
                                                        <StackPanel>
                                                            <TextBlock>
                                                            <Bold>Время и дата совершения преступления (происшествия):</Bold>
                                                            </TextBlock>
                                                            <TextBlock Margin="20,0,0,0" FontSize="7">
                                                            <Italic>- В случае если время, день или месяц совершения не установлены указывается наиболее вероятная дата
                                                            <LineBreak />- Для длящихся и продолжаемых преступлений - указывать время и дату начала совершения преступления</Italic>
                                                            </TextBlock>
                                                        </StackPanel>
                                                        <StackPanel HorizontalAlignment="Right">
                                                            <TextBlock Text="час     мин      число     месяц     год" HorizontalAlignment="Center" FontSize="0.083 in" FontFamily="Times New Roman" />
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Style="{StaticResource ValueStyle}" Text="{Binding F1.CrimeTime.TimeOfDay, Converter={StaticResource TimeConverter}}"
                                                                       FontWeight="Normal" Margin="0,0,10,0" />
                                                                <TextBlock Style="{StaticResource ValueStyle}" Text="{Binding F1.CrimeDate, Converter={StaticResource DateConverter}, ConverterParameter=ShortYear}"
                                                                       FontWeight="Normal" />
                                                            </StackPanel>
                                                        </StackPanel>
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
                                        </TableRow>
                                        <TableRow>
                                            <TableCell>
                                                <Paragraph>
                                                    <DockPanel>
                                                        <TextBlock Width="652">
                                                            <Bold>Преступление обнаружено:</Bold><Underline>следственными органами СК</Underline> военными (01), иными (02);
                                                            <LineBreak /><Underline>прокуратурой</Underline> военной (11), территориальной (12); судом(10);
                                                            <LineBreak />командованием (61), военной полицией (63), <Underline>ревизионными органами:</Underline> ведомственными (65), Счетной палатой (67), Росфинмониторингом (68),  иными (69); налоговыми органами (31);
                                                            <LineBreak /><Underline>сотрудником:</Underline> уголовного розыска (21), Экон.Б и Пр.Корр. (22), наркоконтроля ОВД (23), следственных органов МВД (25), иных служб
                                                            <LineBreak /> ОВД (20), ФСКН (32), таможни (35), ФАС (37),  ВКР ФСБ (41), ФСБ (40), ФСИН (51), иных правоохранительных органов (50);
                                                            <LineBreak />гражданами (71), общественными организациями (72), СМИ (80), иными лицами (90)
                                                        </TextBlock>
                                                        <TextBlock  Text="{Binding F1.CrimeDetectedObject, Converter={StaticResource ValueToSizeConverter}, ConverterParameter=2}"
                                                                    Style="{StaticResource ValueStyle}" />
                                                    </DockPanel>
                                                </Paragraph>
                                            </TableCell>
